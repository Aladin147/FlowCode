{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "html",
    "lcov",
    "json"
  ],
  "report-dir": "coverage",
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/**",
    "src/**/*.test.ts",
    "src/**/*.d.ts",
    "out/**",
    "node_modules/**",
    "coverage/**"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "temp-dir": ".nyc_output",
  "instrument": true,
  "sourceMap": true,
  "produce-source-map": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}